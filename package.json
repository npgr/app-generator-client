{
  "name": "generator",
  "version": "1.0.0",
  "description": "App Generator",
  "main": "app.js",
  "scripts": {
    "start": "electron --enabled-logging app.js",
    "asar": "asar pack App-win32-x64/resources/app App-win32-x64/resources/app.asar",
    "build": "electron-packager . App --platform=win32 --arch=x64 --version=0.35.0 --overwrite && mv App-win32-x64/resources/app/primas App-win32-x64/primas",
	"jscs": "jscs generate.js --preset=airbnb --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://npgr@bitbucket.org/npgr/generator.git"
  },
  "keywords": [
    "App",
    "Generator"
  ],
  "author": "Nuno Goncalves",
  "license": "ISC",
  "homepage": "https://bitbucket.org/npgr/generator#readme",
  "devDependencies": {
    "asar": "^0.8.3",
    "electron-packager": "^5.1.1",
    "electron-prebuilt": "^0.36.0"
  },
  "dependencies": {
    "adm-zip": "^0.4.7",
    "level": "^1.4.0",
    "leveldown": "^1.4.3",
    "levelup": "^1.3.1",
    "lodash": "^4.0.0",
    "sublevel": "^2.4.0"
  }
}
